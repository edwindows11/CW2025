<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.comp2042.MainMenuController"
      prefWidth="600" prefHeight="600" styleClass="root">

    <Button text="Start Game" fx:id="startButton" onAction="#startButtonClicked" style="-fx-font-size: 20px;"/>
    <Button text="Exit" fx:id="exitButton" onAction="#exitButtonClicked" style="-fx-font-size: 20px;"/>

    <stylesheets>
        <URL value="@window_style.css"/>
    </stylesheets>
</VBox>
